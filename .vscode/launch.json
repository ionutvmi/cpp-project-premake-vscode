{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "//localhost/c$/tmp/premake/bin/Debug/StartingProject.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/bin/Debug/",
            "environment": [],
            "preLaunchTask": "Run make build",
            "externalConsole": true,
            "MIMode": "gdb",
            "logging": {
                "engineLogging": false
            },
            "miDebuggerPath": "gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}